module TestMaps;

interface IArray{
	Int sum();
}

class CArray (Int n)implements IArray{
	Int x=10;
	Int i=0;
	Int s=0;
		
	Map<Int,Int> m = EmptyMap;
	
	{
	while (this.i<this.x){
		Int a = this.i*this.n;
		this.m = put(this.m,this.i, a);
		this.i=this.i+1;
	}
	}
	
	Int sum(){
		this.i=0;
		while (this.i<this.x){
			this.s=this.s+ lookupUnsafe(this.m,this.i);	
			this.i=this.i+1;
		}	
		return this.s;
	}
			 
}


{
